// sizeof gives the number of bytes, each int value is composed of two bytes (16 bits)
// there are two values per note (pitch and duration), so for each note there are four bytes
int notes = sizeof(melody) / sizeof(melody[0]) / 2;
 
// this calculates the duration of a whole note in ms
int wholenote = (60000 * 4) / tempo;
 
int divider = 0, noteDuration = 0;
int counter = 0;
 
void setup() {
  // Set the brightness:
  seg.setBrightness(7);
  display.setColorIndex(1);
}

void loop() {
  // demonstrate counter:
  for (counter = 1; counter <= 30; counter++) {
    seg.showNumberDec(counter);  // this knows how to make decimal numbers
    // picture loop
    display.firstPage();
    do {
      draw();
    } while( display.nextPage() );
    delay(250);
  }
  display.firstPage();
  do {
    draw();
  } while( display.nextPage() );
  rickroll();
  while(1);  // after one pass, hang here forever...
}

void draw() {
  // graphic commands to redraw the complete screen should be placed here  
  display.setFont(u8g_font_unifont);
  if (counter == 31) {
    display.drawXBMP( 0, 0, 128, 64, partyFace);
    return;
  }
  int displayBitmap = counter % 10;
  switch (displayBitmap) {
      case 0:
        display.drawXBMP( 0, 0, 128, 64, defaultFace);
        break;
      case 1:
        display.drawXBMP( 0, 0, 128, 64, error);
        break;
      case 2:
        display.drawXBMP( 0, 0, 128, 64, yikesFace);
        break;
      case 3:
        display.drawXBMP( 0, 0, 128, 64, increase);
        break;
      case 4:
        display.drawXBMP( 0, 0, 128, 64, decrease);
        break;
      case 5:
        display.drawXBMP( 0, 0, 128, 64, checkmark);
        break;
      case 6:
        display.drawXBMP( 0, 0, 128, 64, threeCheck);
        break;
      case 7:
        display.drawXBMP( 0, 0, 128, 64, lowThrottle);
        break;
      case 8:
        display.drawXBMP( 0, 0, 128, 64, midThrottle);
        break;
      case 9:
        display.drawXBMP( 0, 0, 128, 64, highThrottle);
        break;
    }  
}

void rickroll() {
  // iterate over the notes of the melody.
  // Remember, the array is twice the number of notes (notes + durations)
  for (int thisNote = 0; thisNote < notes * 2; thisNote = thisNote + 2) {
 
    // calculates the duration of each note
    divider = melody[thisNote + 1];
    if (divider > 0) {
      // regular note, just proceed
      noteDuration = (wholenote) / divider;
    } else if (divider < 0) {
      // dotted notes are represented with negative durations!!
      noteDuration = (wholenote) / abs(divider);
      noteDuration *= 1.5; // increases the duration in half for dotted notes
    }
 
    // we only play the note for 90% of the duration, leaving 10% as a pause
    tone(buzzer, melody[thisNote], noteDuration * 0.9);
 
    // Wait for the specief duration before playing the next note.
    delay(noteDuration);
 
    // stop the waveform generation before the next note.
    noTone(buzzer);
  }
}
 
